---
import Layout from '@/layouts/Layout.astro';
import ServicesPage from '@/components/pages/Services.astro';

const preloadImages = [
	{
		href:"/images/services/empaque.webp",
		as: "image",
		type: "image/webp",
	}
];
import { getI18N } from "@/i18n";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
	title={i18n.services.metaData.title}
	canonical="https://fincaatxona.com.ar"
	metaDescription={i18n.services.metaData.description}
	metaImg="https://fincaatxona.com.ar/images/services/empaque.webp"
	preLoad={preloadImages}
>
	<ServicesPage />
</Layout>
