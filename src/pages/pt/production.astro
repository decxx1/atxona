---
import Layout from '@/layouts/Layout.astro';
import ProductionPage from '@/components/pages/Production.astro';

const preloadImages = [
	{
		href:"/images/production/tractor-con-uvas.webp",
		as: "image",
		type: "image/webp",
	}
];
import { getI18N } from "@/i18n";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
	title={i18n.production.metaData.title}
	canonical="https://fincaatxona.com.ar"
	metaDescription={i18n.production.metaData.description}
	metaImg="https://fincaatxona.com.ar/images/production/tractor-con-uvas.webp"
	preLoad={preloadImages}
>
	<ProductionPage />
</Layout>
