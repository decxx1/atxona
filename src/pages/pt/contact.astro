---
import Layout from '@/layouts/Layout.astro';
import ContactPage from '@/components/pages/Contact.astro';

const preloadImages = [
	{
		href:"/images/index/finca.jpg",
		as: "image",
		type: "image/jpg",
	}
];
import { getI18N } from "@/i18n";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
	title={i18n.contact.metaData.title}
	canonical="https://atxona.com.ar"
	metaDescription={i18n.contact.metaData.description}
	metaImg="https://atxona.com.ar/images/index/hero.jpg"
	preLoad={preloadImages}
>
	<ContactPage />
</Layout>
