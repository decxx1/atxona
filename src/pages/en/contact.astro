---
import Layout from '@/layouts/Layout.astro';
import ContactPage from '@/components/pages/Contact.astro';

const preloadImages = [
	{
		href:"/images/about/cosecha-racimo.webp",
		as: "image",
		type: "image/webp",
	}
];
import { getI18N } from "@/i18n";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
	title={i18n.contact.metaData.title}
	canonical="https://fincaatxona.com.ar/contact"
	metaDescription={i18n.contact.metaData.description}
	metaImg="https://fincaatxona.com.ar/images/about/cosecha-racimo.webp"
	preLoad={preloadImages}
>
	<ContactPage />
</Layout>
