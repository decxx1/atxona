---
import Layout from '@/layouts/Layout.astro';
import MarketingPage from '@/components/pages/Marketing.astro';

const preloadImages = [
	{
		href:"/images/marketing/cosecha-uvas.webp",
		as: "image",
		type: "image/webp",
	}
];
import { getI18N } from "@/i18n";

const { currentLocale } = Astro;
const i18n = getI18N({ currentLocale });
---

<Layout
	title={i18n.marketing.metaData.title}
	canonical="https://grupoatxona.com.ar/marketing"
	alternateEn="https://grupoatxona.com/en/marketing"
	alternatePt="https://grupoatxona.com.ar/pt/marketing"
	metaDescription={i18n.marketing.metaData.description}
	metaImg="https://grupoatxona.com.ar/images/marketing/cosecha-uvas.webp"
	preLoad={preloadImages}
>
	<MarketingPage />
</Layout>
